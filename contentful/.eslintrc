{
    "extends": "airbnb",

    "rules": {
        "func-names": [ "error", "as-needed" ],
        "indent": [ "error", 4 ],
        "one-var": 0,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "array-bracket-spacing": [ "error", "always" ]
    }
}
